{
    "name": "orzcc/laravel-agent",
    "description": "Desktop/mobile user agent parser with support for Laravel, based on Mobiledetect",
    "keywords": ["laravel", "useragent", "agent", "user agent", "browser", "platform", "mobile", "desktop"],
    "homepage": "https://github.com/orzcc/laravel-agent",
    "license" : "MIT",
    "authors": [
        {
            "name": "orzcc",
            "email": "orzcczh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "mobiledetect/mobiledetectlib": "^2.7.6",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0|^6.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Orzcc\\Agent\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Orzcc\\Agent\\AgentServiceProvider"
            ],
            "aliases": {
                "Agent": "Orzcc\\Agent\\Facades\\Agent"
            }
        }
    },
    "suggest": {
        "illuminate/support": "^4.0|^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
